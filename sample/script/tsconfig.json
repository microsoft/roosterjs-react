{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "baseUrl": "../../",
        "jsx": "react",
        "lib": [
            "es5",
            "es2015.promise",
            "dom"
        ],
        "paths": {
            "roosterjs-react/lib/*": [
                "packages/roosterjs-react/lib/*"
